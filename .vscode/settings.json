{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[blade]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.documentSelectors": ["**/*.blade.php"],
    "cSpell.words": ["bayfc", "Nwsl", "Ulids", "vite"],
    // ...
    "search.exclude": {
        "**/.coverage": true,
        "**/.git": true,
        "**/node_modules": true,
        "**/vendor": true
    },
    "files.exclude": {
        "**/.coverage": true
    }
}
